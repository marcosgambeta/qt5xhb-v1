{allmingw}-cflag=-mthreads
-cflag=-Wall
-cflag=-DUNICODE
-cflag=-DQT_DLL
-cflag=-DQT_NO_DEBUG
-cflag=-DQT_THREAD_SUPPORT
-cflag=-DQT_PRINTSUPPORT_LIB
-cflag=-Wno-deprecated-declarations

-plugin=qt5xhb_qtprintsupport.hb

