-hblib

-olib/${hb_plat}/${hb_comp}/hbqtandroidextras5

-workdir=obj

-n

-w3

-gc3

-cpp

-isource/QtAndroidExtras
-iinclude
-i${QT_INC_DIR}

-cflag=-frtti
-cflag=-fexceptions
{allmingw}-cflag=-mthreads
-cflag=-Wall
-cflag=-DUNICODE
-cflag=-DQT_DLL
-cflag=-DQT_NO_DEBUG
-cflag=-DQT_THREAD_SUPPORT
-cflag=-DQT_ANDROIDEXTRAS_LIB
-cflag=-Wno-deprecated-declarations

-plugin=qt5xhb_qtandroidextras.hb

source/QtAndroidExtras/QAndroidActivityResultReceiver.prg
source/QtAndroidExtras/QAndroidBinder.prg
source/QtAndroidExtras/QAndroidIntent.prg
source/QtAndroidExtras/QAndroidJniEnvironment.prg
source/QtAndroidExtras/QAndroidJniExceptionCleaner.prg
source/QtAndroidExtras/QAndroidJniObject.prg
source/QtAndroidExtras/QAndroidParcel.prg
source/QtAndroidExtras/QAndroidService.prg
source/QtAndroidExtras/QAndroidServiceConnection.prg
source/QtAndroidExtras/QtAndroid.prg
source/QtAndroidExtras/QtAndroidExtrasVersion.cpp
