-hblib

-olib/${hb_plat}/${hb_comp}/hbqtvirtualkeyboard5

-workdir=obj

-n

-w3

-gc3

-cpp

-isource/QtVirtualKeyboard
-iinclude
-i${QT_INC_DIR}
-i${QT_INC_DIR}/QtCore

-cflag=-frtti
-cflag=-fexceptions
-cflag=-mthreads
-cflag=-Wall
-cflag=-DUNICODE
-cflag=-DQT_DLL
-cflag=-DQT_NO_DEBUG
-cflag=-DQT_THREAD_SUPPORT
-cflag=-DQT_VIRTUALKEYBOARD_LIB
-cflag=-Wno-deprecated-declarations

-plugin=qt5xhb_qtvirtualkeyboard.hb

source\QtVirtualKeyboard\QtVirtualKeyboardVersion.cpp
