-hblib

-olib/${hb_plat}/${hb_comp}/hbqtsql5

-workdir=obj

-n
-w3
-gc3

-pic

-cpp

-isource/QtSql
-iinclude
-i${QT_INC_DIR}

-cflag=-frtti
-cflag=-fexceptions
{allmingw}-cflag=-mthreads
-cflag=-Wall
-cflag=-DUNICODE
-cflag=-DQT_DLL
-cflag=-DQT_NO_DEBUG
-cflag=-DQT_THREAD_SUPPORT
-cflag=-DQT_SQL_LIB
-cflag=-DQT_WIDGETS_LIB
-cflag=-Wno-deprecated-declarations

-plugin=qt5xhb_qtsql.hb

